{
    "name": "indykoning/grumphp-prettier",
    "description": "A Prettier task for GrumPHP.",
    "type": "plugin",
    "keywords": [
        "indykoning",
        "grumphp",
        "quality",
        "prettier",
        "linter"
    ],
    "homepage": "https://github.com/indykoning/grumphp-prettier",
    "license": "MIT",
    "require": {
        "phpro/grumphp": "^2.0.0"
    },
    "autoload": {
        "psr-4": {
            "Indykoning\\GrumPHPPrettier\\": "src"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpro/grumphp": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
